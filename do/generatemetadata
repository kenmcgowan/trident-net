echo -n "<RepositoryType>git</RepositoryType>" >> $mdfilepath
echo -n "<AssemblyVersion>$kd_version_major.$kd_version_minor.$kd_version_build.$kd_version_patch</AssemblyVersion>" >> $mdfilepath
